# Plug in the joints generated by pg to OpenHRP.
sot.clear

new Selec<vector> cmdLegs
plug pg.walkingerrorposition cmdLegs.in
cmdLegs.selec 0 12	 
unplug featureLegs.errorIN    
plug cmdLegs.out featureLegs.errorIN	
set taskLegs.controlGain -1

sot.push taskLegs
